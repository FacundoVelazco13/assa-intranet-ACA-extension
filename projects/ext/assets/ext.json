{
  "$id": "ext",
  "$version": "1.0.0",
  "$vendor": "Intranet - ASSA extension",
  "$name": "ext",
  "$description": "",
  "$license": "MIT",

  "routes": [
    {
      "id": "intranet.dashboard.route",
      "path": "intranet/dashboard",
      "component": "home-dashboard.component",
      "auth": ["app.auth"]
    },
    {
      "id": "intranet.oym.route",
      "path": "intranet/oym",
      "auth": ["app.auth"],
      "children": [
        {
          "id": "intranet.oym.dashboard.route",
          "path": "",
          "component": "oym-dashboard.component"
        },
        {
          "id": "intranet.oym.procedimientos.route",
          "path": "procedimientos",
          "component": "intranet-page.component",
          "data": {
            "root": "-root-",
            "title": "Procedimientos",
            "sitePath": "Sites/OYM/documentLibrary/Procedimientos"
          }
        },
        {
          "id": "intranet.oym.procedimientos.details.route",
          "path": "procedimientos/details/:nodeId",
          "component": "dossier-view.component",
          "data": {
            "navigateSource": "oym/procedimientos"
          }
        },
        {
          "id": "intranet.oym.politicas.route",
          "path": "politicas",
          "component": "intranet-page.component",
          "data": {
            "root": "-root-",
            "title": "Políticas",
            "sitePath": "Sites/OYM/documentLibrary/politicas"
          }
        },
        {
          "id": "intranet.oym.politicas.details.route",
          "path": "politicas/details/:nodeId",
          "component": "dossier-view.component",
          "data": {
            "navigateSource": "oym/politicas"
          }
        },
        {
          "id": "intranet.oym.formularios.route",
          "path": "formularios",
          "component": "intranet-page.component",
          "data": {
            "root": "-root-",
            "title": "Formularios",
            "sitePath": "Sites/OYM/documentLibrary/Formularios"
          }
        },
        {
          "id": "intranet.oym.formularios.details.route",
          "path": "formularios/details/:nodeId",
          "component": "dossier-view.component",
          "data": {
            "navigateSource": "oym/formularios"
          }
        }
      ]
    }
  ],
  "rules": [
    {
      "id": "ext.rules.isDossier",
      "type": "ext.isDossier"
    },
    {
      "id": "ext.rules.canCreateInCurrentFolder",
      "type": "ext.canCreateInCurrentFolder"
    },
    {
      "id": "ext.rules.isIntranetDocumentLibrary",
      "type": "ext.isIntranetDocumentLibrary"
    },
    {
      "id": "ext.rules.debugContext",
      "type": "ext.debugContext"
    }
  ],
  "features": {
    "icons": [
      {
        "id": "gavel",
        "value": "./assets/images/three-magic-stars-ai.svg"
      },
      {
        "id": "adf:colored-stars-ai",
        "value": "./assets/images/colored-stars-ai.svg"
      }
    ],
    "navbar": [
      {
        "id": "app.navbar.primary",
        "disabled": true
      },
      {
        "id": "app.navbar.secondary",
        "disabled": true
      },
      {
        "id": "intranet.navbar.primary",
        "items": [
          {
            "id": "intranet.navbar.dashboard",
            "order": 100,
            "title": "Inicio",
            "description": "dashboard",
            "route": "intranet/dashboard"
          },
          {
            "id": "intranet.navbar.personal",
            "order": 200,
            "title": "Personal",
            "description": "Gestión de documentos personales",
            "data": {
              "canBeInactive": true
            },
            "children": [
              {
                "id": "app.navbar.personalFiles",
                "order": 100,
                "title": "APP.BROWSE.PERSONAL.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.PERSONAL.SIDENAV_LINK.TOOLTIP",
                "route": "personal-files"
              },
              {
                "id": "app.navbar.libraries.files",
                "order": 200,
                "title": "APP.BROWSE.LIBRARIES.MENU.MY_LIBRARIES.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.LIBRARIES.MENU.MY_LIBRARIES.SIDENAV_LINK.TOOLTIP",
                "route": "libraries"
              },
              {
                "id": "app.navbar.libraries.favorite",
                "order": 300,
                "title": "APP.BROWSE.LIBRARIES.MENU.FAVORITE_LIBRARIES.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.LIBRARIES.MENU.FAVORITE_LIBRARIES.SIDENAV_LINK.TOOLTIP",
                "route": "favorite/libraries"
              },
              {
                "id": "app.navbar.recentFiles",
                "order": 400,
                "title": "APP.BROWSE.RECENT.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.RECENT.SIDENAV_LINK.TOOLTIP",
                "route": "recent-files"
              },
              {
                "id": "app.navbar.favorites",
                "order": 500,
                "title": "APP.BROWSE.FAVORITES.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.FAVORITES.SIDENAV_LINK.TOOLTIP",
                "route": "favorites"
              },
              {
                "id": "app.navbar.shared",
                "order": 600,
                "title": "APP.BROWSE.SHARED.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.SHARED.SIDENAV_LINK.TOOLTIP",
                "route": "shared"
              },
              {
                "id": "app.navbar.trashcan",
                "order": 700,
                "title": "APP.BROWSE.TRASHCAN.SIDENAV_LINK.LABEL",
                "description": "APP.BROWSE.TRASHCAN.SIDENAV_LINK.TOOLTIP",
                "route": "trashcan"
              }
            ]
          },
          {
            "id": "intranet.navbar.documents",
            "order": 300,
            "title": "Documentos",
            "description": "Gestión documental"
          },
          {
            "id": "intranet.navbar.oym",
            "icon": "account_tree",
            "title": "Organización y Métodos",
            "order": 400,
            "route": "intranet/oym",
            "children": [
              {
                "id": "intranet.navbar.oym.dashboard",
                "icon": "home",
                "title": "Dashboard",
                "route": "intranet/oym"
              },
              {
                "id": "intranet.navbar.oym.procedimientos",
                "icon": "schema",
                "title": "Procedimientos",
                "route": "intranet/oym/procedimientos"
              },
              {
                "id": "intranet.navbar.oym.politicas",
                "icon": "gavel",
                "title": "Políticas",
                "route": "intranet/oym/politicas"
              },
              {
                "id": "intranet.navbar.oym.formularios",
                "icon": "assignment",
                "title": "Formularios",
                "route": "intranet/oym/formularios"
              }
            ]
          },
          {
            "id": "intranet.navbar.services",
            "order": 500,
            "title": "Servicios",
            "description": "Gestión de servicios",
            "data": {
              "canBeInactive": true
            },
            "children": [
              {
                "id": "intranet.navbar.customService",
                "order": 200,
                "title": "Custom Service",
                "description": "Custom Service",
                "route": "services"
              }
            ]
          }
        ]
      }
    ],
    "toolbar": [
      {
        "id": "ext.toolbar.create",
        "type": "menu",
        "order": 10,
        "title": "Añadir",
        "description": "Menu de para agregar asociaciones y archivos nuevos",
        "data": {
          "menuType": "flat-button",
          "color": "primary"
        },
        "rules": {
          "visible": ["ext.isDossier", "ext.canCreateInCurrentFolder"]
        },
        "children": [
          {
            "id": "ext.uploadFile",
            "order": 100,
            "title": "Añadir archivos",
            "description": "Añadir archivo al dossier",
            "icon": "attach_file",
            "actions": {
              "click": "UPLOAD_FILES"
            }
          },
          {
            "id": "ext.toolbar.addAssociation",
            "type": "button",
            "order": 200,
            "title": "Añadir asociación",
            "icon": "link",
            "actions": {
              "click": "ADD_ASSOCIATION"
            }
          }
        ]
      },
      {
        "id": "ext.toolbar.createDossier",
        "type": "button",
        "order": 20,
        "title": "Crear Dossier",
        "description": "Crear nuevo dossier",
        "icon": "create_new_folder",
        "data": {
          "color": "primary"
        },
        "actions": {
          "click": "CREATE_DOSSIER"
        },
        "rules": {
          "visible": ["ext.isIntranetDocumentLibrary", "ext.canCreateInCurrentFolder"]
        }
      }
    ]
  }
}
